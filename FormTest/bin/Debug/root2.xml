<?xml version="1.0" encoding="utf-8"?>
<root>
  <NoLR>true</NoLR>
  <Button3D>true</Button3D>
  <Level3D>1</Level3D>
  <TextOverPic>true</TextOverPic>
  <NoFunc>true</NoFunc>
  <BigLR>true</BigLR>
  <Alignment>123</Alignment>
  <fenu name="Machine">
		<escape>
			<link>main</link>
			<visible>true</visible>
		</escape>
		<button>
			<title>Str::Fenu::Machine::CoordinateSwitch</title>
			<position>1</position>
			<state>enable</state>
			<action>DoCoordinateSwitch</action>
			<visible>true</visible>
		</button>
		<button>
			<title>Str::Fenu::Machine::HalfCoordiante</title>
			<position>2</position>
			<state>enable</state>
			<action>DoHalfCoordiante</action>
			<visible>true</visible>
		</button>
		<button>
			<title>Str::Fenu::Machine::ClearCoordinate</title>
			<position>3</position>
			<state>enable</state>
			<action>DoClearCoordinate</action>
			<visible>true</visible>
		</button>
		<button>
			<title>Str::Fenu::Machine::ClearAllCoordinate</title>
			<position>4</position>
			<state>enable</state>
			<action>DoClearAllCoordinate</action>
			<visible>true</visible>
		</button>
		<button name="WorkPiece">
			<title>Str::Fenu::Machine::SetWorkPieceCord</title>
			<position>5</position>
			<state>enable</state>
			<action>ShowMenuWorkPieceCord</action>
			<link>MenuWorkPieceCord</link>
			<visible>true</visible>
		</button>
		<button name="FeatureCoord">
			<action>ShowMenuFeatureCoord</action>
			<position>6</position>
			<title>Str::Fenu::Machine::SetFeatureCoord</title>
			<state>enable</state>
			<link>MenuFeatureCoord</link>
		</button>
	</fenu>
</root>